{
    "name": "Redshift",
    "version": "0.1.2",
    "versionDate": "2021-04-01",
    "author": "hackolade",
    "engines": {
        "hackolade": "4.3.13",
        "hackoladePlugin": "1.2.0"
    },
    "contributes": {
        "target": {
            "applicationTarget": "Redshift",
            "title": "Redshift",
            "versions": []
        },
        "features": {
            "views": {
                "enabled": true,
                "viewLevel": "model",
                "disablePipelines": true
            },
            "nestedCollections": false,
            "disablePatternField": false,
            "enableForwardEngineering": {
                "jsonDocument": true,
                "jsonSchema": true,
                "excel": true,
                "plugin": true
            },
            "enableReverseEngineering": true,
            "disableChoices": true,
            "disableMultipleTypes": {
                "dependencies": [{
                    "dependency": {
                        "level": "parent",
                        "key": "childType",
                        "value": "super"
                    },
                    "value": false
                }],
                "defaultValue": true
            },
            "enableJsonType": true,
            "reverseSchemaIntoOneColumn": true,
            "disableDenormalization": true,
            "enableComplexTypesNormalization": true,
            "enableKeysMultipleAbrr": true,
            "relationships": {
                "compositeRelationships": true
            },
            "FEScriptCommentsSupported": true
        }
    },
    "description": "Hackolade plugin for Amazon Redshift",
    "disabled": false
}
